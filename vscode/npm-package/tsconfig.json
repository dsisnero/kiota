{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "module": "ES6",
    "target": "ES2017",
    "lib": [
      "ES6"
    ],
    "sourceMap": true,
    "rootDir": ".",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "node",
      "jest"
    ],
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
