{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "sourceMap": true,
    "rootDir": ".",
    "outDir": "dist",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "node",
      "jest"
    ],
    "skipLibCheck": true
  }
}
